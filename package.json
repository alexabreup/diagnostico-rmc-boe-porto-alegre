{
  "name": "diagnostico-rmc-poa-elt",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3013",
    "start:clean": "SUPPRESS_WARNINGS=true docusaurus start --port 3013",
    "build": "VALIDATION_STRICT=false node scripts/build-validation.js && docusaurus build",
    "build:netlify": "node scripts/netlify-build.js",
    "build:dev": "VALIDATION_STRICT=false node scripts/build-validation.js && docusaurus build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:fast": "docusaurus build --no-minify",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --project tsconfig.build.json",
    "lint": "eslint src --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "validate-templates": "node scripts/validate-templates.js",
    "validate-content": "node scripts/content-validation.js",
    "validate-all": "npm run validate-templates && npm run validate-content",
    "quality": "npm run lint && npm run format:check && npm run typecheck && npm run test:ci",
    "analyze-bundle": "npm run build:analyze && npx webpack-bundle-analyzer build/static/js/*.js",
    "bundle-analysis": "npm run build && node scripts/bundle-analyzer.js",
    "performance-audit": "npm run build && node scripts/performance-audit.js",
    "performance-monitor": "npm run build && node scripts/performance-monitor.js",
    "optimize-build": "node scripts/optimize-build.js",
    "optimize": "npm run optimize-build"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-ideal-image": "^3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/plugin-client-redirects": "3.8.1",
    "@docusaurus/plugin-pwa": "^3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@swc/core": "^1.3.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/minimatch": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^10.0.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.0",
    "swc-loader": "^0.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "typescript": "~5.6.2",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
